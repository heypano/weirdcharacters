{
    "name": "heypano-web-starter-kit",
    "version": "1.0.1",
    "description": "Pano's JS (Web Dev) starter kit",
    "main": "index.js",
    "config": {
        "server": {
            "port": 3000,
            "port_COMMENT": "This is used by localtunnel and webpack server"
        }
    },
    "scripts": {
        "start": "npm-run-all --parallel security_check open:src lint:watch",
        "open:src": "babel-node buildScripts/srcServer.js",
        "localtunnel": "lt --port $npm_package_config_server_port --subdomain heypano",
        "localtunnel_help": "echo \"Localtunnel exposes port $npm_package_config_server_port ($$npm_package_config_server_port) at a url (https://localtunnel.github.io/www/)\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prebananas": "babel-node buildScripts/prebananas.js",
        "bananas": "babel-node buildScripts/bananas.js",
        "postbananas": "babel-node buildScripts/postbananas.js",
        "security_check": "nsp check",
        "help": "babel-node buildScripts/help.js",
        "lint": "esw --color webpack.config.* src buildScripts",
        "lint:watch": "npm run lint -- --watch",
        "generateMockData": "babel-node buildScripts/generateMockData.js"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "whatwg-fetch": "^2.0.3"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "chai": "^4.1.2",
        "chalk": "^2.3.0",
        "cheerio": "^1.0.0-rc.2",
        "compression": "^1.7.1",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.9",
        "eslint": "^4.17.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-watch": "^3.1.3",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "faker": "^4.1.0",
        "html-webpack-plugin": "^2.30.1",
        "jsdom": "^11.6.2",
        "json-schema-faker": "^0.5.0-rc12",
        "json-server": "^0.12.1",
        "localtunnel": "^1.8.3",
        "mocha": "^5.0.0",
        "nock": "^9.1.6",
        "node-sass": "^4.7.2",
        "npm-run-all": "^4.1.2",
        "nsp": "^3.1.0",
        "numeral": "^2.0.6",
        "open": "^0.0.5",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.1",
        "webpack": "^3.10.0",
        "webpack-dev-middleware": "^2.0.4",
        "webpack-hot-middleware": "^2.21.0",
        "webpack-md5-hash": "0.0.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/heypano/web-starter-kit.git"
    },
    "keywords": [
        "js",
        "starter",
        "kit",
        "npm",
        "babel"
    ],
    "author": "Pano Papadatos (www.heypano.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/heypano/web-starter-kit/issues"
    },
    "homepage": "https://github.com/heypano/web-starter-kit#readme"
}
